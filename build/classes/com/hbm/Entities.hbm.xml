<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">    
    
<hibernate-mapping package="com.entity">
    <!-- 1.用户表 -->
    <class name="User" table="user" >
        <id name="yh_id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property name="yh_code" column="yh_code" type="java.lang.String"/>
        <property name="yh_name" column="yh_name" type="java.lang.String"/>
        <property name="yh_password" column="yh_password" type="java.lang.String"/>
        <property name="yh_type" column="yh_type" type="java.lang.Integer"/>
    </class>    
</hibernate-mapping>